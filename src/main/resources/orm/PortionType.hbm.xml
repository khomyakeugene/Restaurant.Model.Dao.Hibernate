<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.restaurant.model.PortionType" table="portion_type_dic" schema="public"
           catalog="restaurant">
        <cache usage="read-only"/>
        <id name="portionTypeId">
            <column name="portion_type_id" sql-type="integer"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(256)" length="256"/>
        </property>
    </class>
</hibernate-mapping>